{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Active Python File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "envFile": "${workspaceFolder}/.env",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe"
    }
  ]
}